<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" fx:id="rootPane" prefHeight="250.0" prefWidth="300.0"
            xmlns="http://javafx.com/javafx/8" fx:controller="TabScreen.TadScreenController">
    <center>
        <AnchorPane fx:id="TabPane" prefHeight="200.0" prefWidth="300.0" BorderPane.alignment="CENTER">
            <children>
                <TableView fx:id="Tab" layoutX="15.0" prefHeight="190.0" prefWidth="270.0">
                    <columns>
                        <TableColumn fx:id="UserName" editable="false" prefWidth="90.0" resizable="false"
                                     sortable="false" text="UserName"/>
                        <TableColumn fx:id="PassWord" editable="false" prefWidth="90.0" resizable="false"
                                     sortable="false" text="PassWord"/>
                        <TableColumn fx:id="SiteURL" editable="false" prefWidth="90.0" resizable="false"
                                     sortable="false" text="Url"/>
                        <TableColumn fx:id="Select" editable="false" prefWidth="40" resizable="false"
                                     sortable="false"/>
                    </columns>
                </TableView>
            </children>
        </AnchorPane>
    </center>
    <top>
        <AnchorPane prefHeight="50.0" prefWidth="300.0" BorderPane.alignment="CENTER">
            <children>
                <HBox alignment="CENTER" prefHeight="50.0" prefWidth="300.0" spacing="50.0">
                    <children>
                        <Button mnemonicParsing="false" onAction="#Back" prefWidth="55.0" text="Back"/>
                        <Button mnemonicParsing="false" onAction="#Add" prefWidth="55.0" text="Add"/>
                        <Button fx:id="Cancel" mnemonicParsing="false" onAction="#Cancel" prefWidth="55.0"
                                text="Cancel"/>
                    </children>
                </HBox>
            </children>
        </AnchorPane>
    </top>
</BorderPane>
