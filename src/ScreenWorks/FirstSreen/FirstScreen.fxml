<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import Utility.LimitedTextField?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="rootPane" prefHeight="250.0" prefWidth="250.0"
            xmlns="http://javafx.com/javafx/8" fx:controller="ScreenWorks.FirstSreen.FirstScreenController">
    <children>
        <Button fx:id="Open" alignment="CENTER" contentDisplay="CENTER" layoutX="100.0" layoutY="50.0"
                mnemonicParsing="false" onAction="#Open" prefHeight="30.0" prefWidth="50.0" text="Open"
                textAlignment="CENTER">
            <opaqueInsets>
                <Insets/>
            </opaqueInsets>
        </Button>
        <LimitedTextField fx:id="field" layoutX="50.0" layoutY="115.0" promptText="That is a Fild"/>
        <Label layoutX="105.0" layoutY="100.0" text="that is 1"/>
        <Button fx:id="Switch" layoutX="100.0" layoutY="150.0" mnemonicParsing="false" onAction="#Switch"
                prefHeight="30.0" prefWidth="50.0" text="Switch"/>
    </children>
</AnchorPane>
